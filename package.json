{
  "name": "times-up",
  "private": true,
  "version": "1.0.0",
  "description": "multi-timer sequencer for keeping track of time data for multiple sequential tasks.",
  "repository": "https://github.com/johnedisc/times_up.git",
  "author": "johnedisc",
  "license": "MIT",
  "scripts": {
    "start": "nodemon server/dist/server.js",
    "build:client": "pushd client && tsc -p .",
    "build:server": "pushd server/ && tsc --watch --preserveWatchOutput",
    "build": "tsc",
    "dev": "tsc -b --watch --preserveWatchOutput"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
    "typescript": "^5.0.2"
  }
}
